{
  "name": "anyb1s/data",
  "description": "DDD",
  "type": "library",
  "keywords": ["framework"],
  "homepage": "http://google.com",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "AnyB1s\\Data\\": "src/"
    },
    "exclude-from-classmap": ["/Tests/", "/vendor/"]
  },
  "require": {
    "php": "^7.0",
    "beberlei/assert": "^2.7",
    "nilportugues/serializer": "^1.2",
    "ramsey/uuid": "^3.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2"
  },
  "suggest": {
    "ext-pdo_mysql": "For MySQL or MariaDB database storage facility"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
